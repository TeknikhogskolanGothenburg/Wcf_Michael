<?xml version="1.0" encoding="utf-8"?>
<configuration>
 
  <system.serviceModel>
    <services>
      <service name="WCFCarRentalService.CarRentalServices">
        <endpoint address="" binding="basicHttpBinding" contract="WCFCarRentalService.ICarRentalServices">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/WCFCarRentalService/WCFCarRentalService/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  
  
  
  
  
  
</configuration>