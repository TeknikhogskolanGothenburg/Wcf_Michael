<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <system.serviceModel>
    <diagnostics>
      <messageLogging logEntireMessage="true" logMalformedMessages="true"
          logMessagesAtTransportLevel="true" />
    </diagnostics>
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>



      <service name="WCFCarRentalRestService.CarRentalRestService">
        <endpoint address="" binding="basicHttpBinding" contract="WCFCarRentalRestService.ICarRentalRestService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>


        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:7070/Design_Time_Addresses/CarRentalRestService/CarRentalRestService/" />
          </baseAddresses>




        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>
